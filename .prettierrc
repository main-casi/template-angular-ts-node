{
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "as-needed",
  "trailingComma": "none",
  "bracketSpacing": true,
  "arrowParens": "always",
  "overrides": [
      {
          "files": "*.component.html",
          "options": {
              "parser": "angular"
          }
      },
      {
          "files": "*.html",
          "options": {
              "parser": "html"
          }
      }
  ]
}
